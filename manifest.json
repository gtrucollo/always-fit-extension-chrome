{
    "manifest_version": 3,
    "name": "AlwaysFit++",
    "description": "Melhorias na pagina de Atendimentos da AlwaysFit",
    "version": "1.0",
    "action": {
        "default_popup": "index.html",
        "default_icon": "./images/icon.png"
    },
    "content_scripts": [
        {
            "js": [
                "./scripts/app.js"
            ],
            "matches": [
                "https://atendimento.alwaysfitapp.com.br/*"
            ]
        }
    ]
}