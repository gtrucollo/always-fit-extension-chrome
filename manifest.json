{
    "manifest_version": 3,
    "id": "alwaysfit++",
    "name": "AlwaysFit++",
    "description": "Melhorias na pagina de Atendimentos da AlwaysFit",
    "version": "1.0",
    "action": {
        "default_popup": "index.html",
        "default_icon": "./images/icon.png"
    },
    "icons": {
        "16": "./images/icon.png",
        "32": "./images/icon.png",
        "48": "./images/icon.png",
        "128": "./images/icon.png"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "gabrieltrucollo@gmail.com",
            "strict_min_version": "58.0"
        }
    },
    "content_scripts": [
        {
            "js": [
                "./scripts/app.js"
            ],
            "matches": [
                "https://atendimento.alwaysfitapp.com.br/*"
            ]
        }
    ],
    "permissions": [
        "storage"
    ]
}